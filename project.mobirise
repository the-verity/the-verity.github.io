              "min-width": "390px",
              "max-width": "390px",
              "@media (max-width: 767px)": {
                "min-width": "100%",
                "max-width": "100%"
              },
              "&:hover, &:focus": {
                ".slide-content .item-wrapper::before": {
                  "top": "-150px",
                  "right": "-150px"
                }
              },
              ".slide-content": {
                "width": "100%",
                ".item-wrapper": {
                  "position": "relative",
