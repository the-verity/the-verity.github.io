                }
              },
              "&:disabled": {
                "cursor": "default",
                "display": "none"
              },
              "&:hover": {
                "&::before": {
                  "opacity": "0"
                }
              },
              "&.embla__button--prev": {
                "left": "0"
              },
              "&.embla__button--next": {
                "right": "0",
                "@media (max-width: 992px)": {
                  "right": "0"
                }
              }
            },
            ".embla": {
