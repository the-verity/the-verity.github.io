                  "border": "1px solid @border",
                  "border-radius": "20px !important",
                  "padding": "40px 30px",
                  "overflow": "hidden",
                  "&::before": {
                    "content": "''",
                    "position": "absolute",
                    "top": "-400px",
                    "right": "-400px",
                    "width": "300px",
                    "height": "300px",
                    "border-radius": "100%",
                    "background-color": "@active",
                    "filter": "blur(95px)",
                    "transition": "all 0.45s ease-in-out",
                    "@media (max-width: 768px)": {
                      "display": "none"
                    }
                  },
                  ".card-box": {
                    "position": "relative",
                    ".date-wrapper": {
                      "margin-bottom": "15px",
                      "display": "flex",
                      "align-items": "center",
                      "justify-content": "space-between",
                      "@media (max-width: 768px)": {
                        "display": "block"
                      },
                      ".mbr-date": {
                        "margin-right": "10px",
                        "margin-bottom": "0",
                        "@media (max-width: 768px)": {
